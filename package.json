{
	"name": "svelte-toolbox",
	"svelte": "src/components/components.module.js",
	"module": "dist/index.min.mjs",
	"main": "dist/index.min.js",
	"version": "0.0.2",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/svelte-toolbox/svelte-toolbox"
	},
	"devDependencies": {
		"eslint": "~5.16.0",
		"eslint-config-prettier": "~5.0.0",
		"eslint-plugin-svelte3": "^1.2.3",
		"husky": "~2.4.1",
		"lint-staged": "~8.2.1",
		"node-sass": "^4.12.0",
		"npm-run-all": "~4.1.5",
		"prettier": "^1.18.2",
		"prettier-plugin-svelte": "^0.6.0",
		"rollup": "~1.15.6",
		"rollup-plugin-commonjs": "~10.0.0",
		"rollup-plugin-livereload": "~1.0.1",
		"rollup-plugin-node-resolve": "~5.0.3",
		"rollup-plugin-svelte": "~5.1.0",
		"rollup-plugin-terser": "~5.0.0",
		"sirv-cli": "~0.4.4",
		"svelte": "~3.5.1",
		"write": "^1.0.3"
	},
	"scripts": {
		"build": "rollup -c",
		"autobuild": "rollup -c -w",
		"dev": "run-p start:dev autobuild compile:scss wrap:css",
		"start": "sirv public",
		"start:dev": "sirv public --dev",
		"prepublishOnly": "npm run build",
		"lint": "eslint . && prettier --write \"./**\"",
		"lint:test": "eslint . && prettier --check \"./**\"",
		"compile:scss": "sass --watch ./src/styles/mdc.scss ./src/styles/internal/mdc.css --load-path=\"node_modules\"",
		"wrap:css": "node ./src/styles/internal/wrap-css.js"
	},
	"keywords": [
		"svelte"
	],
	"files": [
		"src",
		"dist/index.min.mjs",
		"dist/index.min.js"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js, svelte}": [
			"eslint --fix",
			"git add"
		],
		"*.{css, html, js, json}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@material/textfield": "^2.3.1",
		"material-design-icons": "^3.0.1"
	}
}
